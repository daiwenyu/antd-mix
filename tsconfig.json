{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "test": ["src"],
      "test/*": ["src/*", "*"],
      "antd-mix": ["src"]
    },
    "target": "ES6",
    "module": "ESNext",
    "moduleResolution": "node",
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "lib": ["dom", "ES2022"]
  },
  "include": [".dumi/**/*", ".dumirc.ts", "src/**/*", "docs/**/*"]
}
