{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "antd-mix": ["src"],
      "antd-mix/*": ["src/*", "*"]
    },
    "target": "ES2021",
    "module": "NodeNext"
  },
  "include": [".dumirc.ts", "src/**/*", "docs/**/*"]
}
